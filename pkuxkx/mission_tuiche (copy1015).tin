#var Nickname {伯特子};
#var mydummy {davidss};
#var mytcid {xxkfk};
#alias gttc {hgt ee1388ecd95ac786ac135053d308fc78};
#var tc_count 1;
#alias start_tc
{	
	jiali 0;
