	#ACTION {$Nickname把这批红货送到嘉兴钱庄包方圆那里，他已经派了个伙计名叫%*到%*附近接你} {
	    #var localdesc  {%%2};
	    #var destrid {d1013449893c5a09661043ef64bfe3bc};
