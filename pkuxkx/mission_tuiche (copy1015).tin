	     tell_dummy;
	};		
	#act {^你决定开始跟随}
	{
		#act {^$targetNpc向%*离开。}
		{
			#untick t_tellloc;
			tracehj;
		};
		#act {^这里没有%*}
		{
			#untick t_tellloc;
			tracehj;
		};
		#tick t_tellloc
		{
			start_lochere;
			#act {^located!}
			{
				#unact {^located!};
				#var destrid $hererid;	
				tell $mytcid hjloc $destrid;
				follow $npcename; 
